CREATE TABLE public.movies (
  id serial NOT NULL,
  title text NOT NULL,
  release_date text NOT NULL
);

CREATE TABLE public.actors (
  id serial NOT NULL,
  name text NOT NULL,
  age integer NOT NULL,
  gender text NOT NULL,
  movie integer NOT NULL
);

